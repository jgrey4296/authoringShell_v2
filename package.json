{
    "name": "preact-shell",
    "version": "0.0.1",
    "description": "A preact based UI for JgShell and rete authoring.",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack-dev-server --color",
        "start": "serve build -s -c 1",
        "prestart": "npm run build",
        "build": "cross-env NODE_ENV=production webpack -p --progress",
        "prebuild": "mkdirp build && ncp src/assets build/assets",
        "test": "npm run -s lint && npm run -s test:karma",
        "test:karma": "karma start test/karma.conf.js --single-run",
        "lint": "eslint --fix {src,test}",
        "flow": "flow"
    },
    "keywords": [
        "preact",
        "boilerplate",
        "webpack"
    ],
    "license": "MIT",
    "author": "",
    "devDependencies": {
        "@babel/core": "^7.2.0",
        "@babel/plugin-proposal-class-properties": "^7.2.1",
        "@babel/plugin-syntax-flow": "^7.2.0",
        "@babel/plugin-transform-flow-comments": "^7.2.0",
        "@babel/plugin-transform-flow-strip-types": "^7.2.0",
        "@babel/plugin-transform-react-jsx": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.2.0",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.2.0",
        "@babel/register": "^7.0.0",
        "autoprefixer": "9.4.2",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.4",
        "babel-runtime": "^6.26.0",
        "chai": "^4.2.0",
        "copy-webpack-plugin": "^4.0.1",
        "core-js": "^2.5.7",
        "cross-env": "^5.2.0",
        "css-loader": "1.0.1",
        "eslint": "^5.9.0",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-jest": "^22.1.2",
        "eslint-plugin-react": "^7.0.0",
        "file-loader": "^2.0.0",
        "flow": "^0.2.3",
        "flow-babel-webpack-plugin": "^1.1.1",
        "html-webpack-plugin": "^3.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^23.6.0",
        "jsdoc": "^3.5.5",
        "json-loader": "^0.5.7",
        "less": "^3.9.0",
        "less-loader": "^4.1.0",
        "less-plugin-clean-css": "^1.5.1",
        "mini-css-extract-plugin": "^0.4.5",
        "mkdirp": "^0.5.1",
        "mocha": "^5.2.0",
        "ncp": "^2.0.0",
        "offline-plugin": "^5.0.0",
        "parsimmon": ">=1.2.0",
        "postcss-loader": "^3.0.0",
        "preact-jsx-chai": "^2.2.1",
        "raw-loader": "^0.5.1",
        "regenerator-runtime": "^0.13.1",
        "replace-bundle-webpack-plugin": "^1.0.0",
        "script-ext-html-webpack-plugin": "^2.0.0",
        "sinon": "7.1.1",
        "sinon-chai": "^3.3.0",
        "source-map-loader": "^0.2.1",
        "style-loader": "^0.23.1",
        "url-loader": "^1.0.0",
        "webpack": "^4.26.1",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.10"
    },
    "dependencies": {
        "preact": "^8.1.0",
        "preact-compat": "^3.15.0",
        "preact-render-to-string": "^4.1.0",
